{
  "name": "electron-slide-beacon",
  "version": "0.3.3",
  "description": "Electron app to broadcast URLs as an Eddystone Physical Web beacon",
  "main": "main.js",
  "scripts": {
    "package": "node ./node_modules/electron-packager/cli.js ./ PhysicalWebBroadcast --platform=darwin --arch=x64 --version=1.3.1 --ignore=node_modules/electron-rebuild --asar --icon=icon.icns",
    "electron": "node ./node_modules/electron-prebuilt/cli.js .",
    "rebuild": "node ./node_modules/.bin/electron-rebuild"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/dermike/electron-slide-beacon.git"
  },
  "keywords": [
    "electron"
  ],
  "author": "Mikael Jergefelt",
  "license": "ISC",
  "devDependencies": {
    "electron-packager": "7.3.0",
    "electron-prebuilt": "1.3.1",
    "electron-rebuild": "1.1.5"
  },
  "dependencies": {
    "mdns": "2.3.3",
    "eddystone-beacon": "1.0.6",
    "ws": "1.1.1"
  }
}
