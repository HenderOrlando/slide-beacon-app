{
  "name": "electron-slide-beacon",
  "version": "0.2.1",
  "description": "Electron app to broadcast URLs as an Eddystone Physical Web beacon",
  "main": "main.js",
  "scripts": {
    "package": "node ./node_modules/electron-packager/cli.js ./ EddystoneURL --platform=darwin --arch=x64 --version=0.36.12 --ignore=node_modules/electron-rebuild --asar --icon=icon.icns",
    "electron": "node ./node_modules/electron-prebuilt/cli.js ."
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/dermike/electron-slide-beacon.git"
  },
  "keywords": [
    "electron"
  ],
  "author": "Mikael Jergefelt",
  "license": "ISC",
  "devDependencies": {
    "electron-packager": "6.0.0",
    "electron-prebuilt": "0.36.12",
    "electron-rebuild": "1.1.3"
  },
  "dependencies": {
    "mdns": "2.3.1",
    "eddystone-beacon": "1.0.6",
    "ws": "1.0.1"
  }
}
